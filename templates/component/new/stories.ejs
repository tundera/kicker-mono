---
to: <%= h.prependComponentsPath("<%= location ? `src/${location}` : 'src' %>/<%= h.camelizedBaseName(name) %>/<%= h.camelizedBaseName(name) %>.stories.tsx") %>
---
<% component = h.camelizedBaseName(name) -%>
<% storyProps = `${component}Props` -%>
import { Story, Meta } from '@storybook/react'

import { <%= component %>, <%= component %>Props } from './<%= component %>'
// import * as DependentStories from './Dependent.stories'

export default {
  title: '<%= component %>',
  component: <%= component %>,
} as Meta

const Template: Story<<%= component %>Props> = (args) => <<%= component %> {...args} />

export const Primary = Template.bind({})
Primary.args = {
  //   ...DependentStories.Primary.args,
}
