{
  "name": "@tunderadev/cli",
  "description": "Kicker CLI",
  "version": "0.1.0",
  "author": {
    "name": "Philip Johnston",
    "email": "tunderadev@gmail.com",
    "url": "https://github.com/tundera"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/kicker-mono.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "main": "dist/tunderadev-cli.cjs.js",
  "module": "dist/tunderadev-cli.esm.js",
  "typings": "dist/tunderadev-cli.cjs.d.ts",
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./src/commands",
    "bin": "kicker",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-autocomplete"
    ],
    "hooks": {
      "init": []
    }
  },
  "bin": {
    "kicker": "./bin/run"
  },
  "bugs": "https://github.com/tundera/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "0.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "1.2.4",
    "@oclif/plugin-update": "1.3.10",
    "cli-ux": "5.5.1",
    "cross-spawn": "7.0.3",
    "eslint-plugin-mdx": "^1.11.0",
    "execa": "5.0.0",
    "listr": "0.14.3",
    "ora": "5.4.0",
    "prettier": "^2.2.1",
    "tslib": "^2.1.0",
    "yarn-or-npm": "3.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "1.2.8",
    "@types/cross-spawn": "6.0.2",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "eslint": "7.22.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "globby": "^10",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
